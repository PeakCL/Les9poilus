{
    "name": "La Communauté des 9 Poilus",
    "version": "1.0.0",
    "description": "développement et intégration web sur-mesure",
    "main": "tailwind.config.js",
    "scripts": {
        "build:tailwind": "npx tailwindcss -i ./src/input.css -o ./assets/css/output.css --minify",
        "watch:tailwind": "npx tailwindcss -i ./src/input.css -o ./assets/css//output.css --watch",
        "jekyll:serve": "bundle exec jekyll serve",
        "jekyll:build": "bundle exec jekyll build",
        "start": "npm-run-all --parallel watch:tailwind jekyll:serve",
        "prebuild": "npx update-browserslist-db@latest --update-db || true",
        "build": "npm run build:tailwind && npm run jekyll:build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Kimakito/La Communauté des 9 Poilus.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "type": "commonjs",
    "bugs": {
        "url": "https://github.com/Kimakito/La Communauté des 9 Poilus/issues"
    },
    "homepage": "https://github.com/Kimakito/La Communauté des 9 Poilus#readme",
    "dependencies": {
        "browserslist": "^4.25.0",
        "dotenv": "^16.5.0",
        "electron-to-chromium": "^1.5.167",
        "escalade": "^3.2.0",
        "fraction.js": "^4.3.7",
        "jquery": "^3.7.1",
        "nanoid": "^3.3.11",
        "node-fetch": "^3.3.2",
        "node-releases": "^2.0.19",
        "normalize-range": "^0.1.2",
        "picocolors": "^1.1.1",
        "postcss-value-parser": "^4.2.0",
        "source-map-js": "^1.2.1",
        "update-browserslist-db": "^1.1.3"
    },
    "devDependencies": {
        "@tailwindcss/typography": "^0.5.16",
        "autoprefixer": "^10.4.x",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.x.x",
        "postcss-cli": "^10.x.x",
        "tailwindcss": "3.4.4"
    }
}
